{
  "name": "eav-scenes-web",
  "version": "0.1.0",
  "type": "module",
  "description": "EAV Scenes Web - Scene planning and shot list management for production workflows",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run test",
    "supabase:types": "npx supabase gen types typescript --project-id zbxvjyrbkycbfhwmmnmy --schema public > src/types/database.types.ts",
    "supabase:link": "npx supabase link --project-ref zbxvjyrbkycbfhwmmnmy",
    "supabase:pull": "npx supabase db pull",
    "db:update": "npm run supabase:pull && npm run supabase:types"
  },
  "dependencies": {
    "@elevanaltd/shared-lib": "^0.1.8",
    "@elevanaltd/ui": "^0.3.0",
    "@supabase/supabase-js": "^2.76.1",
    "@tanstack/react-query": "^5.90.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.2",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^17.2.2",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.2",
    "jsdom": "^23.0.1",
    "typescript": "^5.2.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "undici": "^5.28.6",
    "path-to-regexp": "^6.2.3",
    "esbuild": "^0.25.10"
  }
}
